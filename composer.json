{
    "name": "wachterjohannes/debug-mcp-resources",
    "description": "Documentation resources for PHP and Symfony development",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "modelcontextprotocol/php-sdk": "^0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Wachterjohannes\\DebugMcp\\Resources\\": "src/"
        }
    },
    "extra": {
        "wachterjohannes/debug-mcp": {
            "classes": [
                "Wachterjohannes\\DebugMcp\\Resources\\PhpDocumentationResource",
                "Wachterjohannes\\DebugMcp\\Resources\\SymfonyDocumentationResource"
            ]
        }
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix"
    }
}
